<% include ./partials/header %>
<h1>INDEX PAGE</h1>

<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.titel %></h2>
        <img src="<%= blog.image %>" alt="">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
        <a href="/blogs/<%=blog._id %>">Read More</a>
    </div>
<% }) %>

<% include ./partials/footer %>